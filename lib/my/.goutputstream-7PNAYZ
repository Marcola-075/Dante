#include <stddef.h>
#include <stdlib.h>
#include <unistd.h>
#include <stdio.h>
#include <sys/types.h>
#include <sys/stat.h>
#include <fcntl.h>
#include <string.h>

char *my_realloc(char *str, char *str_two, int i, int y);

char *my_cat(char *filepath)
{
    int a;
    int rd = 0;
    char *buff = malloc(sizeof(char) * 10);
    
    if (filepath == NULL)
		return (NULL);
	a = open(filepath, O_RDONLY);
	if (a < 0)
		return (NULL);
	while ((rd != read(a, buff, 9)) != 0)
			write(1, buff, rd);
		close(a);
		return (NULL);
			
	
}
